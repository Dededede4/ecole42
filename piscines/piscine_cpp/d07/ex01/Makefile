CC=clang++

CFLAGS=-Wall -Werror -Wextra

RM=rm -f

SRC= iter.cpp

OBJ= iter

all: iter

clean:
	@($(RM) $(OBJ))

fclean: clean

re: fclean all

.PHONY: clean fclean re
